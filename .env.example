# Django Settings - Copy this file to .env and fill in your values
# DO NOT commit .env to version control!

# Generate a new secret key with: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"
DJANGO_SECRET_KEY=your-secret-key-here

# Set to False in production
DJANGO_DEBUG=True

# Comma-separated list of allowed hosts (e.g., example.com,www.example.com)
DJANGO_ALLOWED_HOSTS=
